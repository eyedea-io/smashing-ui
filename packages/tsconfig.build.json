{
  "compilerOptions": {
    "composite": true,
    "declaration": true,

    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "jsx": "react",

    "strict": true,
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "pretty": true
  }
}
